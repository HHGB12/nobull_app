<p id="notice"><%= notice %></p>

<h1>Website Contents</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Founder</th>
      <th>Founded</th>
      <th>About us</th>
      <th>Background story</th>
      <th>Team</th>
      <th>Services</th>
      <th>Pricing</th>
      <th>Message from founder</th>
      <th>Other info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @website_contents.each do |website_content| %>
      <tr>
        <td><%= website_content.user %></td>
        <td><%= website_content.founder %></td>
        <td><%= website_content.founded %></td>
        <td><%= website_content.about_us %></td>
        <td><%= website_content.background_story %></td>
        <td><%= website_content.team %></td>
        <td><%= website_content.services %></td>
        <td><%= website_content.pricing %></td>
        <td><%= website_content.message_from_founder %></td>
        <td><%= website_content.other_info %></td>
        <td><%= link_to 'Show', website_content %></td>
        <td><%= link_to 'Edit', edit_website_content_path(website_content) %></td>
        <td><%= link_to 'Destroy', website_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Website Content', new_website_content_path %>
