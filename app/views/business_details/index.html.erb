<p id="notice"><%= notice %></p>

<h1>Business Details</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Business address</th>
      <th>Mailing address</th>
      <th>Physical or mailing</th>
      <th>Other address info</th>
      <th>Business phone</th>
      <th>Business phone2</th>
      <th>Business phone3</th>
      <th>Business fax</th>
      <th>Business phone info</th>
      <th>Business email address</th>
      <th>Business email address2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_details.each do |business_detail| %>
      <tr>
        <td><%= business_detail.user %></td>
        <td><%= business_detail.business_address %></td>
        <td><%= business_detail.mailing_address %></td>
        <td><%= business_detail.physical_or_mailing %></td>
        <td><%= business_detail.other_address_info %></td>
        <td><%= business_detail.business_phone %></td>
        <td><%= business_detail.business_phone2 %></td>
        <td><%= business_detail.business_phone3 %></td>
        <td><%= business_detail.business_fax %></td>
        <td><%= business_detail.business_phone_info %></td>
        <td><%= business_detail.business_email_address %></td>
        <td><%= business_detail.business_email_address2 %></td>
        <td><%= link_to 'Show', business_detail %></td>
        <td><%= link_to 'Edit', edit_business_detail_path(business_detail) %></td>
        <td><%= link_to 'Destroy', business_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business Detail', new_business_detail_path %>
